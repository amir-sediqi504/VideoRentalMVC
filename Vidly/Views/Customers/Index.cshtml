@model List<Vidly.Models.Customer>
<!DOCTYPE html>
<html>
<head>
    <title>Customers</title>
</head>
<body>
    <h1>Customers</h1>
    <table class="table">
        <thead>
            <tr>
                
                <th>Customer</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var customer in Model)
            {
                <tr>
                    
                    <td>@customer.Name</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
